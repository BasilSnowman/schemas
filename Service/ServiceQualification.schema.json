{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "ServiceQualification.schema.json", 
    "title": "ServiceQualification", 
    "type": "object", 
    "properties": {
        "description": {
            "type": "string"
        }, 
        "effectiveQualificationDate": {
            "$ref": "Date.schema.json/#Date"
        }, 
        "estimatedResponseDate": {
            "$ref": "Date.schema.json/#Date"
        }, 
        "expectedQualificationDate": {
            "$ref": "Date.schema.json/#Date"
        }, 
        "expirationDate": {
            "$ref": "Date.schema.json/#Date"
        }, 
        "href": {
            "type": "string"
        }, 
        "id": {
            "type": "string"
        }, 
        "provideAlternative": {
            "type": "boolean"
        }, 
        "provideOnlyAvailable": {
            "type": "boolean"
        }, 
        "provideUnavailabilityReason": {
            "type": "boolean"
        }, 
        "serviceQualificationdateTime": {
            "type": "string", 
            "format": "date-time"
        }, 
        "state": {
            "type": "string"
        }, 
        "qualificationItem": {
            "type": "array", 
            "items": {
                "$ref": "ServiceQualificationItem.schema.json/#ServiceQualificationItem"
            }
        }, 
        "relatedParty": {
            "type": "array", 
            "items": {
                "$ref": "RelatedPartyRef.schema.json/#RelatedPartyRef"
            }
        }
    }, 
    "allOf": [
        {
            "$ref": "CommonComponents.schema.json/#polymorphicPattern"
        }
    ]
}