{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "OrganizationParentRelationship.schema.json", 
    "title": "OrganizationParentRelationship", 
    "definitions": {
        "OrganizationParentRelationship": {
            "type": "object", 
            "description": "Parent references of an organization in a structure of organizations.", 
            "properties": {
                "id": {
                    "type": "string", 
                    "description": "Unique identifier of the parent organization"
                },                
				"href": {
                    "type": "string", 
                    "description": "Link to the parent organization"
                }, 
                "relationshipType": {
                    "type": "string", 
                    "description": "Type of the relationship. Could be juridical, hierarchical, geographical, functional,\u2026"
                }, 
                "validFor": {
                    "$ref": "TimePeriod.schema.json#/definitions/TimePeriod"
                }
            }, 
            "required": [
                "id"
            ]
        }
    }
}
