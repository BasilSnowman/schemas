{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "OrganizationChildRelationship.schema.json", 
    "title": "OrganizationChildRelationship", 
    "definitions": {
        "OrganizationChildRelationship": {
            "type": "object", 
            "description": "Child references of an organization in a structure of organizations.", 
            "properties": {
                "id": {
                    "type": "string", 
                    "description": "Unique identifier of child organization"
                }, 			
                "href": {
                    "type": "string", 
                    "description": "Link to the child organization"
                }, 
                "relationshipType": {
                    "type": "string", 
                    "description": "Type of the relationship. Could be juridical, hierarchical, geographical, functional,\u2026"
                }, 
                "validFor": {
                    "$ref": "TimePeriod.schema.json#/definitions/TimePeriod"
                }
            }, 
            "required": [
                "id"
            ]		
        }
    }
}
