{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "PartyRef.schema.json", 
    "title": "PartyRef", 
    "definitions": {
        "PartyRef": {
            "type": "object", 
            "description": "", 
            "properties": {
                "href": {
                    "type": "string", 
                    "description": "Reference of the party"
                }, 
                "id": {
                    "type": "string", 
                    "description": "Unique identifier of the party"
