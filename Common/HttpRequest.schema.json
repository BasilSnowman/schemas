{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "HttpRequest.schema.json", 
    "title": "HttpRequest", 
    "definitions": {
        "HttpRequest": {
            "$id": "#HttpRequest", 
            "type": "object", 
            "properties": {
                "body": {
                    "type": "string"
                }, 
                "header": {
                    "type": "array", 
                    "items": {
                        "$ref": "HeaderItem.schema.json#/definitions/HeaderItem"
                    }, 
                    "title": "header"
                }, 
                "method": {
                    "type": "string"
                }, 
                "to": {
                    "type": "string"
                }
            }, 
            "required": [
                "body", 
                "header"
            ]
        }
    }
}
