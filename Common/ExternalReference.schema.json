{
    "$schema": "http://json-schema.org/draft-07/schema#", 
    "$id": "ExternalReference.schema.json", 
    "title": "ExternalReference", 
    "definitions": {
        "ExternalReference": {
            "type": "object", 
            "description": "External reference of the individual or reference in other system", 
            "properties": {
                 "externalReferenceType": {
                    "type": "string", 
                    "description": "Type of the external reference"
                }, 
                "url": {
                    "type": "string", 
                    "description": "Link to the external reference"
                }, 
                "value": {
                    "type": "string", 
                    "description": "External reference value"
                }
            },
			"allOf": [
                {
                    "$ref": "Entity.schema.json#Entity"
                }
            ]			
        }
    }
}