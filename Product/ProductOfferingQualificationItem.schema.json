{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ProductOfferingQualificationItem.schema.json",
    "title": "ProductOfferingQualificationItem",
    "definitions": {
        "ProductOfferingQualificationItem": {
            "type": "object",
            "properties": {
                "expectedActivationDate": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Date when the requester look for productOfferingQualification activation"
                },
                "id": {
                    "type": "string",
                    "description": "Id of the productOfferingQualification item"
                },
                "qualificationItemResult": {
                    "type": "string",
                    "description": "Qualification result for this productOfferingQualification item. It could be:\n\n qualified (request productOffering or productSpecification are available), \n unqualified (requested not available and not alternate available),\n alternate (requested not available but proposal available)\n"
                },
                "state": {
                    "type": "string",
                    "description": "State of the productOfferingQualification item (acknowledged, inProgress, terminatedWithError, done)"
                },
                "eligibilityUnavailabilityReason": {
                    "type": "array",
                    "items": {
                        "$ref": "../Customer/EligibilityUnavailabilityReason.schema.json/#definitions/EligibilityUnavailabilityReason"
                    }
                },
                "category": {
                    "$ref": "../Customer/CategoryRef.schema.json/#definitions/CategoryRef"
                },
                "qualificationItemRelationship": {
                    "type": "array",
                    "items": {
                        "$ref": "../Service/QualificationItemRelationship.schema.json/#definitions/QualificationItemRelationship"
                    }
                },
                "note": {
                    "type": "array",
                    "items": {
                        "$ref": "../Common/Note.schema.json/#definitions/Note"
                    }
                },
                "product": {
                    "$ref": "Product.schema.json/#definitions/Product"
                },
                "terminationError": {
                    "type": "array",
                    "items": {
                        "$ref": "../Customer/TerminationError.schema.json/#definitions/TerminationError"
                    }
                },
                "productOffering": {
                    "$ref": "ProductOfferingRef.schema.json/#definitions/ProductOfferingRef"
                },
                "alternateProductOfferingProposal": {
                    "type": "array",
                    "items": {
                        "$ref": "AlternateProductOfferingProposal.schema.json/#definitions/AlternateProductOfferingProposal"
                    }
                }
            },
            "allOf": [
                {
                    "$ref": "../Common/CommonComponents.schema.json/#definitions/polymorphicPattern"
                }
            ]
        }
    }
}
