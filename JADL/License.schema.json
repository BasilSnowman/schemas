{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "License.schema.json",
  "title": "License",
  "definitions": {
    "License": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The license name",
          "example": "MyLicense"
        },
        "url": {
          "type": "string",
          "format": "uri",
          "description": "The license URL",
          "example": "http://mylicense.com"
        }
      }
    }
  }
}