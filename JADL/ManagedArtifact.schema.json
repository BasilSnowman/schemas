{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ManagedArtifact.schema.json",
  "definitions": {
    "ManagedArtifact": {
      "$id": "#ManagedArtifact",
      "type": "object",
      "description": "General information about a managed artifact",
      "properties": {
        "description": {
          "type": "string",
          "description": "The description for the artifact",
          "example": ""
        },
        "version": {
          "$ref": "TestVersion.schema.json#/definitions/TestVersion"
        },
        "attributes": {
          "type": "array",
          "items": {
            "$ref": "Attribute.schema.json#/definitions/Attribute"
          }
        },
        "relatedParties": {
          "type": "array",
          "items": {
            "$ref": "RelatedParty.schema.json#/definitions/RelatedParty"
          }
        },
        "termsOfService": {
          "type": "string",
          "description": "The terms of service for the artifact",
          "example": "My terms of service"
        },
        "state": {
          "type": "string",
          "description": "The state of the artifact",
          "example": "locked"
        },
        "contact": {
          "$ref": "Contact.schema.json#/definitions/Contact"
        },
        "license": {
          "$ref": "License.schema.json#/definitions/License"
        }
      },
      "allOf": [
        {
          "$ref": "ResourceEntity.schema.json#/ResourceEntity"
        }
      ]
    }
  }
}

