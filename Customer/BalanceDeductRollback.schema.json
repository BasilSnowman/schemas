                },
                "id": {
                    "type": "string",
                    "description": "Unique Identifier within the server for the rollback deduction  operation request"
                },
                "requestedDate": {
                    "type": "string",
                    "format": "date-time",
                    "description": "Date when the rollback deduction request was received in the server"
                },
                "status": {
